{
  "name": "starter2",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-preset-env": "^1.1.8",
    "cross-env": "^3.2.3",
    "eslint": "^3.17.0",
    "eslint-find-rules": "^1.14.3",
    "eslint-plugin-flowtype": "^2.30.2",
    "eslint-plugin-html": "^2.0.1",
    "eslint-plugin-react": "^6.10.0",
    "eslint-plugin-volkmann": "^0.3.3",
    "flow-bin": "^0.41.0",
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.2",
    "prettier-eslint-cli": "^3.1.2",
    "react-scripts": "0.9.3",
    "sass-lint": "^1.10.2"
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "url-search-params": "^0.6.1"
  },
  "scripts": {
    "build": "npm run build-css && react-scripts build",
    "build-css": "node-sass src/ -o src/",
    "coverage": "cross-env CI=true npm test -- --coverage",
    "efr": "eslint-find-rules --unused ~/.eslintrc.json --no-error",
    "lint": "eslint --quiet src __tests__ --ext .js,.jsx",
    "flow": "flow",
    "format": "prettier-eslint --write \"src/**/*.js*\"",
    "postbuild": "cd build && npm install --production",
    "prepush": "npm run verify",
    "postcommit": "npm run format",
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "verify": "npm run lint && cross-env CI=true npm test",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch"
  }
}
