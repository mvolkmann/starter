{
  "name": "starter2",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-preset-env": "^1.1.8",
    "cross-env": "^3.2.3",
    "enzyme": "^2.7.1",
    "eslint": "^3.17.0",
    "eslint-find-rules": "^1.14.3",
    "eslint-plugin-flowtype": "^2.30.3",
    "eslint-plugin-html": "^2.0.1",
    "eslint-plugin-react": "^6.10.0",
    "eslint-plugin-volkmann": "^0.3.3",
    "flow-bin": "^0.41.0",
    "husky": "^0.13.2",
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.2",
    "prettier-eslint-cli": "^3.1.2",
    "react-addons-test-utils": "^15.4.2",
    "react-scripts": "0.9.5",
    "react-test-renderer": "^15.4.2",
    "stylelint": "^7.9.0"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "compression": "^1.6.2",
    "express": "^4.15.2",
    "font-awesome": "^4.7.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-fontawesome": "^1.5.0",
    "url-search-params": "^0.6.1"
  },
  "scripts": {
    "_format": "prettier-eslint --write \"src/**/*.js\"",
    "build": "npm run build-css && react-scripts build",
    "build-css": "node-sass src/ -o src/",
    "cover": "npm-run-all cover:*",
    "cover:gen": "cross-env CI=true npm test -- --coverage",
    "cover:open": "open coverage/lcov-report/index.html",
    "efr": "eslint-find-rules --unused .eslintrc.json --no-error",
    "flow": "flow",
    "fmt": "npm-run-all -s link-project _format",
    "fmt-dev": " npm-run-all -s link-dev _format",
    "link-dev": "ln -sf ~/.eslintrc.json .eslintrc.json",
    "link-project": "ln -sf .eslintrc.project.json .eslintrc.json",
    "lint": "npm-run-all lint:**",
    "lint:js": "eslint --quiet src __tests__ --ext .js,.jsx",
    "lint:scss": "stylelint \"src/*.scss\"",
    "postbuild": "cd build && npm install --production",
    "postcommit": "npm run _format",
    "preinstall": "yarn global add nodemon",
    "prepush": "npm run verify",
    "start": "npm-run-all -p watch-css start-js",
    "start-prod": "sudo nodemon ./server.js",
    "start-js": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "verify": "npm run lint && cross-env CI=true npm test",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch",
    "dbuild": "docker build -t launchpad_ui .",
    "drun": "docker run --name ui -d -p 3000:3000 launchpad_ui"
  }
}
